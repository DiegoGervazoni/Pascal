program eitalasqueira;
uses crt;
var
  num200 : integer;
  num1200 : integer;
  num4000 : integer;
  num6000 : integer;

  resultado : integer;
begin

  num200 := 200;
  num1200 := 1200;
  num4000 := 4000;
  num6000 := 6000;

  resultado := num200+num1200+num4000+num6000;
  writeln (resultado);
  readkey;
end.

