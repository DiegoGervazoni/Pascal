program tomelhorando;
uses crt;
var
  bailarina222 : integer;
  bailarina333 : integer;
  bailarina444 : integer;
  bailarina555 : integer;

  resultado : integer;

begin
  bailarina222 := 400;
  bailarina333 := 600;
  bailarina444 := 800;
  bailarina555 := 950;

  resultado := bailarina222+bailarina333+bailarina444+bailarina555;
  writeln (resultado);
  readkey;
end.

